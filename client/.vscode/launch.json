{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Flutter Web",
      "request": "launch",
      "type": "dart",
      "args": [
        "--web-port=8080"
      ],
      "program": "lib/main.dart",
      "cwd": "${workspaceFolder}"
    },
    {
      "name": "Flutter Web (Chrome)",
      "request": "launch",
      "type": "dart",
      "args": [
        "--device-id=chrome",
        "--web-port=8080"
      ],
      "program": "lib/main.dart",
      "cwd": "${workspaceFolder}"
    },
    {
      "name": "Flutter Windows",
      "request": "launch",
      "type": "dart",
      "args": [],
      "program": "lib/main.dart",
      "cwd": "${workspaceFolder}"
    }
  ]
}